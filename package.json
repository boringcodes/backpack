{
  "name": "@boringcodes/backpack",
  "version": "1.3.1",
  "description": "Backpack with Some Boring Features",
  "homepage": "https://github.com/boringcodes/backpack",
  "bugs": "https://github.com/boringcodes/backpack/issues",
  "license": "MIT",
  "author": "BoringCodes <hi@boringcodes.com> (https://boringcodes.com)",
  "repository": "https://github.com/boringcodes/backpack.git",
  "bin": {
    "backpack": "./bin/backpack"
  },
  "scripts": {
    "purge": "rm -rf node_modules",
    "format": "prettier --write --ignore-path .gitignore config/**/*.js",
    "format:check": "prettier --check --ignore-path .gitignore config/**/*.js",
    "lint": "eslint --ignore-path .gitignore config/**/*.js",
    "release": "standard-version --no-verify",
    "release:major": "yarn release --release-as major",
    "release:minor": "yarn release --release-as minor",
    "release:patch": "yarn release --release-as patch"
  },
  "dependencies": {
    "cross-spawn": "^7.0.1",
    "dotenv-webpack": "^1.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.2",
    "ramda": "^0.27.0",
    "source-map-support": "^0.5.12",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.2.2",
    "webpack": "^4.35.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@boringcodes/eslint-config": "^1.0.0",
    "@boringcodes/prettier-config": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "standard-version": "^7.1.0"
  }
}
